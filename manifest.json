{
    "name": "Pomodoro Timer",
    "description": "Productivity assitant",
    "version": "1.0",
    "manifest_version": 3,
    "icons" :{
        "16": "/src/16.png",
        "32": "/src/32.png",
        "48": "/src/48.png",
        "128": "/src/128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
]
}